{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": 0,
    "quotes": 0,
    "arrow-body-style": 0,
    "object-curly-newline": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "no-else-return": 0,
    "react/jsx-curly-brace-presence": 0,
    "react/jsx-one-expression-per-line": 0,
    "jsx-a11y/label-has-associated-control": 0
  }
}
